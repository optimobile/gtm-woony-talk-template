___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const injectScript = require('injectScript');
const encodeUriComponent = require('encodeUriComponent');
const queryPermission = require('queryPermission');
const widgetBundleLoaderUrl = 'https://optimobile.se/downloads/installation/woony.widget.install.production.js?widgetVer=' + data.version + '&licenseKey=' + encodeUriComponent(data.licenseKey) + '&debugLogging=' + data.debugLogging;

const onSuccess = () => {
  log('Woony Widget : Successfully loaded');
  data.gtmOnSuccess();
};

const onFailure = (e) => {
  log('Woony Widget : An error occured during loading');
  log(e);
  data.gtmOnSuccess();
};

log('Checking permission to load script:' + widgetBundleLoaderUrl);
if (queryPermission('inject_script', widgetBundleLoaderUrl)) {
  log('Permission check: OK');
  injectScript(widgetBundleLoaderUrl, onSuccess, onFailure);
} else {
  log('Permission check: FAIL');
  data.gtmOnFailure();
}

log('Woony Widget : Initializing...');
log(widgetBundleLoaderUrl);
injectScript(widgetBundleLoaderUrl, onSuccess, onFailure, widgetBundleLoaderUrl);

data.gtmOnSuccess();


